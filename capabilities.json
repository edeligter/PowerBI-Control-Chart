{
    "dataRoles": [
        {
            "displayName": "X Axis Data",
            "name": "xAxis",
            "kind": "Grouping",
            "description": "Select X Axis data that is either numeric or date-based"
        },
        {
            "displayName": "Y Axis (Measured) Data",
            "name": "yAxis",
            "kind": "Measure",
            "description": "Select Y Axis data that is numeric",
            "requiredTypes": [
                {
                    "numeric": true
                }
            ]
        },
        {
            "displayName": "Stage Names",
            "name": "stagecolumn",
            "kind": "Measure",
            "description": "Select column that identifies stage names"
        }
    ],
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "xAxis": {
                        "min": 1,
                        "max": 1
                    },
                    "yAxis": {
                        "max": 1
                    },
                    "stagecolumn": {
                        "min": 0,
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "xAxis"
                    },
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "yAxis"
                            }
                        },
                        {
                            "bind": {
                                "to": "stagecolumn"
                            }
                        }
                    ]
                }
            }
        }
    ],
